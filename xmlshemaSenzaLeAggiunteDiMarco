					<xsd:element name="idFKV" type="tId"/>
					<xsd:element name="idFKUR" type="tId" />
					<xsd:element name="idFKAm" type="tId" />
					<xsd:element name="abiltato" type="xsd:boolean"/><!--false=commento inserito dall'utente ma non ancora 
          										 approvato, true=commento approvato e visibile a
											 tutti-->
					<xsd:element name="voto" type="xsd:integer" /> 
					<xsd:element name="Testo" type="tTesto" />
			</xsd:complexType>

			<xsd:complexType name="tNews">
					<xsd:element name="idPKN" type="tId" />
					<xsd:element name="idFKAm" type="tId" />
					<xsd:element name="testo" type="tTesto" />
			</xsd:complexType>

			<xsd:complexType name="tFaq">
					<xsd:element name="idPKF" type="tId" />
					<xsd:element name="idFKAm" type="tId" />
					<xsd:element name="argomento" type="xsd:string" />
					<xsd:element name="testo" type="tTesto" />
			</xsd:complexType>

			<xsd:complexType name="tServizio">	
					<xsd:element name="idPKS" type="tId" />
					<xsd:element name="descrizione" type="xds:string" />
					<xsd:element name="prezzo" type="xsd:float" />  
			</xsd:complexType>
			
			<xsd:complexType name="tTesto">	
					<!--
					<xsd:element name="idPKT" type="tId" /> !
					!! testi sono un tipo astratto, non hanno bisogno di un id???-->
					<xsd:element name="data" type="xsd:date" />
					<xsd:element name="titolo" type="xsd:string" />
					<xsd:element name="contenuto" type="xds:string" />
			</csd:complexType>

			<xsd:complexType nome="tTabServizioPrenotato"> 	
					<xsd:element nome="idPKSP" type="tId" />
					<xsd:element nome="idFKP" type="tId" />
					<xsd:element nome="idFKS" type="tId" />
			</xsd:complexType>

			<xsd:element name="tId" type="xsd:integer" />

			<!--dovrebbe essere guisto ho preso dagli appunti della prof modificando cosÃ¬ che fosse un modello tende alla 
			veneziana  .... forse si potrebbe enumerare tutti i casi su tNumGiorno? o collegarli + chiaramente?-->
		
			<xsd:type name="tGiorno">
				<xsd:union>
					<xsd:element name="numGiorno" type="tNumGiorno" />
					<xsd:element nome="nomeGiorno" type="tNomeGiorno" />			
				</xsd:union>
			</xsd:type>

			<xsd:type name="tNumGiorno">
				<xsd:restriction base="xsd:integer">
					<xsd:minExclusive value="1"/>
					<xsd:maxExclusive value="7"/>
				</xsd:restriction>
			</xsd:type>	

			<xsd:type name="tNomeGiorno">
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="lunedi"/>
					<xsd:enumeration value="martedi"/>				
					<xsd:enumeration value="mercoledi"/>
					<xsd:enumeration value="giovedi"/>
					<xsd:enumeration value="venerdi"/>				
					<xsd:enumeration value="sabato"/>
					<xsd:enumeration value="domenica"/>
				</xsd:restriction>
			</xsd:type>
			
			<xsd:simpleType name="tCodiceFiscale" >
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="[A-Z]{6}[0-9]{2}[A-Z][0-9]{2}[A-Z][0-9]{3}[A-Z]" />
				</xsd:restriction>
			</xsd:simpleType>
			
			<xsd:simpleType name="tPassword" >
				<xsd:restriction base="xsd:string" >
					<xds:pattern value="[a-zA-Z]*[0-9][a-zA-Z0-9]*" minLength="8" />
				</xsd:restriction>
	</xs:schema>
